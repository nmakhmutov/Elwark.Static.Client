{
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://+:5006",
        "Protocols": "Http1"
      }
    }
  },
  "Postgresql": {
    "ConnectionString": "Host=localhost;Database=world;Username=master_account;Password=master_password"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "CorrelationId": "Error",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.AspNetCore.Hosting.Diagnostics": "Information",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext"
    ]
  }
}
